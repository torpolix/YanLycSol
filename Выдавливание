import math


class Square:
    def __init__(self, side):
        self.side = side

    def extrude(self, h):
        return self.side * self.side * h


class Rectangle:
    def __init__(self, side1, side2):
        self.side1 = side1
        self.side2 = side2

    def extrude(self, h):
        return self.side1 * self.side2 * h


class Triangle:
    def __init__(self, side):
        self.side = side

    def extrude(self, h):
        return (math.sqrt(3) / 4) * self.side * self.side * h


class Circle:
    def __init__(self, radius):
        self.radius = radius

    def extrude(self, h):
        return math.pi * self.radius * self.radius * h
