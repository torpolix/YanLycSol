class Student:
    def __init__(self, name, university):
        self.name = name
        self.university = university
        self.year = 1

    def get_name(self):
        return self.name

    def get_university(self):
        return self.university

    def get_year(self):
        return self.year

    def study(self):
        if self.year < 6:
            self.year += 1


class Employee:
    growth_levels = ['junior', 'middle', 'senior', 'lead']

    def __init__(self, name, company):
        self.name = name
        self.company = company
        self.position_index = 0

    def get_name(self):
        return self.name

    def get_company(self):
        return self.company

    def work(self):
        if self.position_index < len(Employee.growth_levels) - 1:
            self.position_index += 1

    def get_position(self):
        return Employee.growth_levels[self.position_index]


class Human:
    def __init__(self, name):
        self.name = name

    def get_name(self):
        return self.name
