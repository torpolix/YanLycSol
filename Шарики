import json

# Считываем данные из файла input.json
with open('input.json', 'r') as file:
    data = json.load(file)

# Сортируем шарики сначала по количеству цветов, затем по радиусу,
# затем по удаленности от начала координат и, наконец, по id
sorted_data = sorted(data, key=lambda x: (len(x['colors']), x['radius'], x['x'] ** 2 + x['y'] ** 2, x['id']),
                     reverse=True)

# Выводим id шариков в порядке уменьшения количества цветов, радиусов,
# удаления от начала координат и id
for ball in sorted_data:
    print(ball['id'], end=' ')
